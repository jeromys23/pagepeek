{
    "manifest_version": 3,
    "name": "PagePeek",
    "description": "An extension that allows users to see hidden inputs on web pages",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "sidePanel"],
    "host_permissions": [
      "<all_urls>"
     ],
    "side_panel": {
      "default_path": "Popup/SidePanel.html"
    },
    "action": {
      "default_popup": "Popup/Popup.html"
    },
    "icons": {
      "16": "Utils/Images/Icons/icon16.png",
      "32": "Utils/Images/Icons/icon32.png",
      "48": "Utils/Images/Icons/icon48.png",
      "128": "Utils/Images/Icons/icon128.png"
    }
  }